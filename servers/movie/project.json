{
  "root": "servers/movie",
  "sourceRoot": "servers/movie/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["./dist"],
      "options": {
        "cwd": "servers/movie",
        "command": "yarn lint"
      }
    },

    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["./coverage"],
      "options": {
        "cwd": "servers/movie",
        "command": "yarn test"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["build/packages/js"],
      "options": {
        "cwd": "servers/movie",

        "command": "yarn build"
      }
    }
  },
  "tags": []
}
