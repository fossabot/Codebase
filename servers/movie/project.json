{
  "root": "servers/movie",
  "sourceRoot": "servers/movie/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "servers/movie",
        "command": "yarn lint"
      }
    },

    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["./coverage"],
      "options": {
        "cwd": "servers/movie",
        "command": "yarn test"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["dist/servers/movie"],
      "options": {
        "cwd": "servers/movie",
        "command": "yarn build"
      },
      "configurations": {
        "production": {
          "cwd": "servers/movie",
          "command": "yarn build --webpack"
        },
        "development": {
          "cwd": "servers/movie",
          "command": "yarn build --watch"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["dist/servers/movie/out-tsc"],
      "options": {
        "cwd": "servers/movie",

        "command": "yarn start:dev"
      }
    }
  },
  "tags": []
}
